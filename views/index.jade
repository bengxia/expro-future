!=partial('stores/sidebar')
style
    select, input[type="file"] {
        height: 18px;
        line-height: 28px;
    }
#content
  .panel
    .header
      ul.breadcrumb
        li
          a(href='/') 主页
          span.divider /
        - if(locals.current_user) {
        li
          a(href='/signup') 设置
          span.divider /
        li
          a(href='/signout') 退出
          span.divider /
        - }
        li.active #{config.description}-首页
    table(border='1', width='100%')
        tr
            td
                - if(locals.current_user) {
                <iframe name="content_frame" marginwidth=0 marginheight=0 width=100% height=800 src="http://192.168.0.102/zentaopms/www/" frameborder=0></iframe>
                - }else{
                <iframe name="content_frame" marginwidth=0 marginheight=0 width=100% height=800 src="http://localhost:10080/signin" frameborder=0></iframe>
                - }
